gfxmenu = /gfx-theme2
splashimage = /splash.xpm.gz
foreground = white
background = light-gray


title Memtest ISO
find --set-root /ISO/memtest.iso
map /ISO/memtest.iso (hd32)
map --hook
root (hd32)
chainloader (hd32)
boot

title Pts Stress 2010.1
find --set-root /ISO/pts.2010.1.iso
map /ISO/pts.2010.1.iso (hd32)
map --hook
root (hd32)
chainloader (hd32)
boot

title Memtest +
kernel /ISO/mt86plus
boot



title Loading XP RAM install 
find --set-root /ISO/Win_XP_Home_SP3_OEM.iso 
map --mem /ISO/Win_XP.ima (fd0) 
#(fd1) optional, required at some BIOS 
#map --mem /ISO/Win_XP.ima (fd1) 
map --mem  /ISO/Win_XP_Home_SP3_OEM.iso (0xFF) 
map (hd0) (hd1) 
map (hd1) (hd0) 
map --hook 
chainloader (0xFF)/I386/SETUPLDR.BIN




title <-- back 
find --set-root /menu.lst
configfile /menu.lst


